<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard">
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/scripts/**</from>
		<to>/scripts/$1</to>
	</rule>
	<rule>
		<from>/styles/**</from>
		<to>/styles/$1</to>
	</rule>
	<rule>
		<from>/output/maxent/**</from>
		<to>/output/maxent/$1</to>
	</rule>
	<rule>
		<from>/output/sampling/**</from>
		<to>/output/sampling/$1</to>
	</rule>
	<rule>
		<from>/analysis</from>
		<to>/index.zul</to>
	</rule>
	<rule>
		<from>/maxent2.zul</from>
		<to>/maxent2.zul</to>
	</rule>
	<rule>
		<from>/maxent2</from>
		<to>/maxent2.zul</to>
	</rule>
	<rule>
		<from>/maxentw</from>
		<to>/maxentwizard.zul</to>
	</rule>
	<rule>
		<from>/maxentwizard.zul</from>
		<to>/maxentwizard.zul</to>
	</rule>
	<rule>
		<from>/speciesList.zul</from>
		<to>/speciesList.zul</to>
	</rule>
	<rule>
		<from>/sampling.zul</from>
		<to>/sampling.zul</to>
	</rule>
	<rule>
		<from>/ALOC.zul</from>
		<to>/ALOC.zul</to>
	</rule>
	<rule>
		<from>/filtering_cl.html</from>
		<to>/filtering_cl.html</to>
	</rule>
	<rule>
		<from>/tabulation.zul</from>
		<to>/tabulation.zul</to>
	</rule>
	<rule>
		<from>/images/**</from>
		<to>/images/$1</to>
	</rule>
	<rule>
		<from>/slider/**</from>
		<to>/slider/$1</to>
	</rule>
	<rule>
		<from>/zkau/**</from>
		<to>/zkau/$1</to>
	</rule>
	<rule>
		<from>/zkau</from>
		<to>/zkau</to>
	</rule>
	<rule>
		<from>/**</from>
		<to>/app/$1</to>
	</rule>
	<outbound-rule>
		<from>/app/**</from>
		<to>/$1</to>
	</outbound-rule>
</urlrewrite>
