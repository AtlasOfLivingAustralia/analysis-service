<tns:portal xsi:schemaLocation="xmlbeans.config.portal.emii.org.au             ../../../../PortalDatasourceConfigurationFile/src/main/xsd/PortalDatasourceConfigurationFile.xsd " xmlns:tns="xmlbeans.config.portal.emii.org.au" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <!--Settings
    ========-->
  <tns:settings>
    <!--Default selections used for blank portal sessions-->
    <!--default bounding box - portal zooms here on loading-->
    <tns:defaultBoundingBox>
      <tns:minLatitude>-44.0</tns:minLatitude>
      <tns:maxLatitude>-10.0</tns:maxLatitude>
      <tns:minLongitude>110.0</tns:minLongitude>
      <tns:maxLongitude>155.0</tns:maxLongitude>
    </tns:defaultBoundingBox>
    <!--disable looking up depth/altitude for points clicked on map - if enabled, requires
        access to a massive database...-->
    <tns:disableDepthServlet>true</tns:disableDepthServlet>
    <!--'Well known' values    Values that the system expects to be able to find on load-->
    <!--Mime type to be treated as XML - normaly application/xml-->
    <tns:xmlMimeType>application/xml</tns:xmlMimeType>
    <!--Configuration file will be reread after this interval (ms)
                Handy values:
                60000 		= 1 minute
                3600000 	= 1 hour
                86400000 	= 24 hours-->
    <tns:configRereadInterval>86400000</tns:configRereadInterval>
    <!--On first application load, the config file will be re-read after this
        interval and will subsequently be reread after the config_reread_interval.

        This is to handle servers that are not fully up yet being restarted
        at the same time as us-->
    <tns:configRereadInitialInterval>120000</tns:configRereadInitialInterval>
    <!--GeoNetwork supported countries xml file (from web/geonetwork/loc/ru/xml/countries.xml)
        in the GeoNetwork source tree - must be within classpath.  This is used to generate
        the list of countries in the registration system.  Don't forget leading '/' !-->
    <tns:isoCountriesFilename>/country_codes.xml</tns:isoCountriesFilename>
    <!--Network timeouts
        ~~~~~~~~~~~~~~~~
        Connection timeouts for connecting to servers - used to prevent hung
        connections when a server is down-->
    <!--maximum time to wait for a connection to establish under normal
        conditions (ms)-->
    <tns:netConnectTimeout>1000</tns:netConnectTimeout>
    <!--maximum time to wait for reading from a connection to finish
        under normal conditions(ms)-->
    <tns:netReadTimeout>10000</tns:netReadTimeout>
    <!--maximum time to wait for a connection to establish under slow
        server conditions (ms)-->
    <tns:netConnectSlowTimeout>600000</tns:netConnectSlowTimeout>
    <!--maximum time to wait for reading from a connection to finish
        under slow server conditions (ms)-->
    <tns:netReadSlowTimeout>600000</tns:netReadSlowTimeout>
    <!--Proxy server
        ~~~~~~~~~~~~-->
    <!--is proxy server required?-->
    <tns:proxyRequired>false</tns:proxyRequired>
    <!--proxy server host or IP address - do not prefix with http://..!-->
    <tns:proxyHost>proxy.utas.edu.au</tns:proxyHost>
    <!--proxy server port number-->
    <tns:proxyPort>8080</tns:proxyPort>
    <!--proxy server username-->
    <tns:proxyUsername>bjward</tns:proxyUsername>
    <!--proxy server password-->
    <tns:proxyPassword>LEgEndrE</tns:proxyPassword>
    <!--proxy will NOT be contacted for hosts listed here.  Content of this
        element must be a string of hosts delimited by the pipe character '|'
        eg: host1.com|host2.com|host2.com

        You will probably want to use a CDATA block for this-->
    <tns:proxyNonProxyHosts></tns:proxyNonProxyHosts>
    <!--remote request servlet will only allow requests to be forwarded to these
        hosts.   Content of this element must be a string of hosts delimited by
        the pipe character '|' eg: host1.com|host2.com|host2.com

        You will probably want to use a CDATA block for this-->
    <tns:proxyAllowedHosts>emii3.its.utas.edu.au|
            ows.bom.gov.au|
            data.aims.gov.au|
            obsidian|
            obsidian.bluenet.utas.edu.au|
            test.emii.org.au|
            www.imos.org.au|
            emii3.its.utas.edu.au|
            opendap-ivec.arcs.org.au|
            imos.aodn.org.au|
            imos0.ersa.edu.au|
            imos1.ersa.edu.au|
            imos2.ersa.edu.au|
            spatial-dev.ala.org.au|
            spatial.ala.org.au|
            ec2-184-73-34-104.compute-1.amazonaws.com</tns:proxyAllowedHosts>
    <!--Cache Reflector
        ~~~~~~~~~~~~~~~

        RemoteRequest servlet setup - optionally alters URLs so that they are requested
        through a local servlet, magically caching them with squid if configured
        correctly-->
    <!--Relative URL for the cache reflector service-->
    <tns:cacheUrl>/RemoteRequest</tns:cacheUrl>
    <!--Name of the Parameter containing the URL for requests made to
        the cache reflector service-->
    <tns:cacheParameter>url</tns:cacheParameter>
    <!--Time to keep an object in cache (SQUID) in seconds-->
    <tns:cacheMaxAge>86400</tns:cacheMaxAge>
    <!--MEST configurations-->
    <tns:mestConfigurations>
      <tns:mestConfiguration>
        <tns:id>mest-1.2.x</tns:id>
        <tns:name>mest 1.2 configuration</tns:name>
        <tns:disabled>false</tns:disabled>
        <tns:description>emii mest configuration for geonetwork 1.2.x</tns:description>
        <tns:version>1.2.x</tns:version>
        <!--paths-->
        <!--Path to keyword web service - normally /geonetwork/srv/en/portal.search.keywords-->
        <tns:servicePathKeyword>/geonetwork/srv/en/portal.search.keywords</tns:servicePathKeyword>
        <!--path to the login service - normally /geonetwork/srv/en/xml.user.login for MEST/geonetwork-->
        <tns:servicePathLogin>/geonetwork/srv/en/xml.user.login</tns:servicePathLogin>
        <!--path to the self registration service - normally /geonetwork/srv/en/xml.self.register for MEST/geonetwork-->
        <tns:servicePathSelfRegistration>/geonetwork/srv/en/xml.self.register</tns:servicePathSelfRegistration>
        <!--path to the user info service - normally /geonetwork/srv/en/xml.user.get for MEST/geonetwork-->
        <tns:servicePathUserInfo>/geonetwork/srv/en/xml.user.get</tns:servicePathUserInfo>
        <!--path to reset password service - normally /geonetwork/srv/en/xml.password.emaillink-->
        <tns:servicePathResetPassword>/geonetwork/srv/en/xml.password.emaillink</tns:servicePathResetPassword>
        <!--path to change password service - normally /geonetwork/srv/en/xml.password.change-->
        <tns:servicePathChangePassword>/geonetwork/srv/en/xml.password.change</tns:servicePathChangePassword>
        <!--tokens-->
        <!--Error token sent by MEST when a duplicate user has been detected.  Normally
                errorEmailAddressAlreadyRegistered-->
        <tns:tokenDuplicateUser>errorEmailAddressAlreadyRegistered</tns:tokenDuplicateUser>
        <!--Error token sent by MEST when incorrect username/password detected.  Normally
                UserLoginEx-->
        <tns:tokenIncorrectLogin>UserLoginEx</tns:tokenIncorrectLogin>
        <!--Error token sent from MEST when the changekey has expired.  Normally
                "Change key invalid" (without the quotes)-->
        <tns:tokenExpiredChangeKey>Change key invalid</tns:tokenExpiredChangeKey>
        <!--Error token sent from MEST to indicate user does not exist.  Normally UserNotFoundEx-->
        <tns:tokenInvalidUser>UserNotFoundEx</tns:tokenInvalidUser>
      </tns:mestConfiguration>
    </tns:mestConfigurations>
    <!--Supplementary
        ~~~~~~~~~~~~~
        This section is where you can add arbitrary additional configuration information
        without having to worry about modifying the XML schema and regenerating the
        xmlbeans.

        Configuration of system messages and icons has been moved to a separate
        .properties file to keep the size of this config file manageable-->
    <tns:supplementary>
      <!--internal id for the user defined layers group-->
      <user_defined_layer_group_id>mylayers-0</user_defined_layer_group_id>
      <!--Buffer in pixels for getFeatureInfo requests-->
      <get_feature_info_buffer>5</get_feature_info_buffer>
      <!--Buffer zone in TILES around the current viewport

            * W * A * R * N * I * N * G * ! *
            Increasing this number DRAMATICALLY increases load
            on both browsers and servers!  You have been warned.-->
      <openlayers_tile_buffer>1</openlayers_tile_buffer>
      <!--Openlayer: images wider and taller than the tile size by a value of 2 x gutter
            NOT WORKING  over the date line. incorrect values sent to server or Geoserver not handling send values.
            Keep as zero till fixed-->
      <openlayers_tile_gutter>0</openlayers_tile_gutter>
      <!--fixed width of animated images when requesting from ncwms/thredds-->
      <animation_width>512</animation_width>
      <!--fixed height of animated images when requesting from ncwms/thredds-->
      <animation_height>512</animation_height>
      <!--maximum length (chars) of a layer name for display purposes.  Names
            exceeding this limit will be truncated-->
      <layer_name_max_length>80</layer_name_max_length>
      <!--Default width for left portal menu-->
      <menu_default_width>380px</menu_default_width>
      <!--Width of the menu when minimised-->
      <menu_minimised_width>22px</menu_minimised_width>
      <!--There is no built in limit for thredds animations but the whole
            server will start running slowly if users request too many frames
            for animation - selecting a whole year's worth of data takes
            about 7 minutes at the moment, so we impose an arbitrary frame limit
            here to protect the server and for THREDDS-->
      <thredds_maximum_allowed_frames>30</thredds_maximum_allowed_frames>
      <!--height of the north borderlayer component (banner)-->
      <north_border_height>100px</north_border_height>
      <!--size in pixels to leave when stacking windows - an integer (=px)-->
      <window_stacking_boundary>50</window_stacking_boundary>
      <!--size after which the window stacking will roll over again
            to prevent drawing too far off screen - an integer (=px)-->
      <window_stacking_constant>600</window_stacking_constant>
      <!-- GeoServer details -->
      <geoserver_url>http://localhost:8080</geoserver_url>
      <geoserver_username>admin</geoserver_username>
      <geoserver_password>password</geoserver_password>
      <!-- ALA Spatial Analysis Toolkit -->
      <sat_url>http://spatial-dev.ala.org.au</sat_url>
      <!-- Common Name search url -->
      <!-- replace the _query_ with the common name query string -->
      <common_name_url>http://data.ala.org.au/taxonomy/taxonName/ajax/returnType/commonName/view/ajaxTaxonName?query=_query_</common_name_url>
      <!-- (printing) command for imagemagick convert -->
     <convert_cmd>/usr/bin/convert -quiet</convert_cmd>
     <!-- (printing) command for wkhtmltoimage -->
     <wkhtmltoimage_cmd>/data/ala/utils/wkhtmltoimage/wkhtmltoimage-amd64</wkhtmltoimage_cmd>
     <!-- (printing) local path for print output, also web accessible -->
     <print_output_path>/usr/local/tomcat/instance_03_webportal/webapps/webportal/print/</print_output_path>
     <!-- (printing) url for print_output_path -->
     <print_output_url>http://spatial-dev.ala.org.au/webportal/print/</print_output_url>
     <!-- (printing) server url -->
     <print_server_url>http://spatial-dev.ala.org.au/webportal/</print_server_url>
    <!-- url to species metadata.  _lsid_ is replaced with actual lsid -->
     <species_metadata_url>http://bie.ala.org.au/species/_lsid_</species_metadata_url>
     <help_url>http://test.ala.org.au</help_url>
    </tns:supplementary>
  </tns:settings>
