<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class dynamic-insert="false" dynamic-update="false" mutable="true"
		name="au.org.emii.portal.userdata.UserMap" optimistic-lock="version"
		polymorphism="implicit" select-before-update="false">
		<id name="id" type="long">
			<column name="id" />
			<generator class="assigned" />
		</id>
		<one-to-one name="bBox" class="au.org.emii.portal.BoundingBox" />
		<many-to-one name="currentBaseLayer" class="au.org.emii.portal.MapLayer" column="currentBaseLayerId" unique="true"/>
		<property name="maximised" type="boolean">
			<column name="maximised" />
		</property>
		<property name="currentNavigationTab" type="integer">
			<column name="currentNavigationTab" />
		</property>
		<property name="currentView" type="integer">
			<column name="currentView" />
		</property>
	</class>
</hibernate-mapping>
