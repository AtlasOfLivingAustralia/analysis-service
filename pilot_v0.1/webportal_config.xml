<?xml version="1.0" encoding="UTF-8"?>
<tns:portal xmlns:tns="config.portal.emii.org.au"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="config.portal.emii.org.au ../../../../PortalDatasourceConfigurationFile/src/main/xsd/PortalDatasourceConfigurationFile.xsd ">
	<tns:configuration>

		<!--
			This section is where you can add arbitrary additional configuration
			information without having to worry about modifying the XML schema
			and regenerating the xmlbeans. Configuration of system messages and
			icons has been moved to a separate .properties file to keep the size
			of this config file manageable
		-->

		<!--
			maximum time to wait for a connection to establish under normal
			conditions (ms)
		-->
		<net_connect_timeout>6000</net_connect_timeout>

		<!--
			maximum time to wait for reading from a connection to finish under
			normal conditions(ms)
		-->
		<net_read_timeout>20000</net_read_timeout>

		<!--
			maximum time to wait for a connection to establish under slow server
			conditions (ms)
		-->
		<net_connect_slow_timeout>600000</net_connect_slow_timeout>

		<!--
			maximum time to wait for reading from a connection to finish under
			slow server conditions (ms)
		-->
		<net_read_slow_timeout>600000</net_read_slow_timeout>


		<!-- internal id for the user defined layers group -->
		<user_defined_layer_group_id>mylayers-0</user_defined_layer_group_id>

		<!--
			Configuration file will be reread after this interval (ms) Handy
			values: 60000 = 1 minute 3600000 = 1 hour 86400000 = 24 hours
		-->
		<config_reread_interval>360000</config_reread_interval>

		<!--
			On first application load, the config file will be re-read after this
			interval and will subsequently be reread after the
			config_reread_interval. This is to handle servers that are not fully
			up yet being restarted at the same time as us
		-->
		<config_reread_initial_interval>120000
		</config_reread_initial_interval>

		<!--
			proxyHack() will be called in ConfigurationLoader if true, and all
			HTTP requests will then be proxied unless the host is listed in the
			proxy_non_proxy_hosts element
		-->
		<proxy_required>false</proxy_required>

		<!-- proxy server host or IP address -->
		<proxy_host>proxy.utas.edu.au</proxy_host>

		<!-- proxy server port number -->
		<proxy_port>8080</proxy_port>

		<!-- proxy server username -->
		<proxy_username>bjward</proxy_username>

		<!-- proxy server password -->
		<proxy_password>LEgEndrE</proxy_password>

		<!--
			proxy will NOT be contacted for hosts listed here, delimited by
			commas and enclosed in quotes
		-->
		<proxy_non_proxy_hosts><![CDATA[
		'localhost',
		'emii3.its.utas.edu.au',
		'imos.aodn.org.au',
		'data.ala.org.au',
		'maps.ala.org.au'
	]]>
		</proxy_non_proxy_hosts>

		<!--
			python proxy script also reads this file and will only allow requests
			to be forwarded to these hosts, delimited by ','
		-->
		<proxy_allowed_hosts><![CDATA[
		'ows.bom.gov.au',
		'data.aims.gov.au',
		'test.emii.org.au',
		'www.openlayers.org', 		
		'openlayers.org',
		'labs.metacarta.com', 
		'world.freemap.in',
		'prototype.openmnnd.org', 
		'geo.openplans.org',
        'sigma.openplans.org',
        'www.marine-geo.org',
        'www.openstreetmap.org',
        't1.hypercube.telascience.org',
        'rolexsydneyhobart.com',
        'www.imos.org.au',
        'emii3.its.utas.edu.au',
        'opendap-ivec.arcs.org.au',
        'imos.aodn.org.au',
        'data.ala.org.au',
        'maps.ala.org.au'
     ]]></proxy_allowed_hosts>

		<!-- Buffer in pixels for getFeatureInfo requests -->
		<get_feature_info_buffer>5</get_feature_info_buffer>

		<!--
			Buffer zone in TILES around the current viewport * W * A * R * N * I
			* N * G * ! * Increasing this number DRAMATICALLY increases load on
			both browsers and servers! You have been warned.
		-->
		<openlayers_tile_buffer>1</openlayers_tile_buffer>

		<!--
			Openlayer: images wider and taller than the tile size by a value of 2
			x gutter NOT WORKING over the date line. incorrect values sent to
			server or Geoserver not handling send values. Keep as zero till fixed
		-->
		<openlayers_tile_gutter>0</openlayers_tile_gutter>

		<!-- MEST URI path to search keywords -->
		<mest_keyword_path>/geonetwork/srv/en/portal.search.keywords
		</mest_keyword_path>

		<!--
			fixed width of animated images when requesting from ncwms/thredds
		-->
		<animation_width>512</animation_width>

		<!--
			fixed height of animated images when requesting from ncwms/thredds
		-->
		<animation_height>512</animation_height>

		<!--
			maximum length (chars) of a layer name for display purposes. Names
			exceeding this limit will be truncated
		-->
		<layer_name_max_length>37</layer_name_max_length>

		<!-- 
  	Default width for left portal menu
  	-->
		<menu_default_width>380px</menu_default_width>

		<!-- 
	Width of the menu when minimised
	-->
		<menu_minimised_width>22px</menu_minimised_width>

		<!--
			There is no built in limit for thredds animations but the whole
			server will start running slowly if users request too many frames for
			animation - selecting a whole year's worth of data takes about 7
			minutes at the moment, so we impose an arbitrary frame limit here to
			protect the server and for THREDDS
		-->
		<thredds_maximum_allowed_frames>30</thredds_maximum_allowed_frames>

		<!--  height of the north borderlayer component (banner) -->
		<north_border_height>100px</north_border_height>

		<!--
			size in pixels to leave when stacking windows - an integer (=px)
		-->
		<window_stacking_boundary>50</window_stacking_boundary>

		<!--
			size after which the window stacking will roll over again to prevent
			drawing too far off screen - an integer (=px)
		-->
		<window_stacking_constant>600</window_stacking_constant>

		<!--  
	Time to keep an object in cache (SQUID) in seconds
	 -->
		<cache_max_age>86400</cache_max_age>

		<!-- 
	URL to hit for indirect cache requests
	-->
		<cache_url>/webportal/RemoteRequest</cache_url>

		<!--
			paramter to set for indirect cache requests (contains the url to go
			to)
		-->
		<cache_parameter>url</cache_parameter>

	</tns:configuration>

